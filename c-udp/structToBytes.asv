function bytes = structToBytes(myStruct)

bytes = uint8.empty;

structFieldNames = fieldnames(myStruct);
for i = 1 : numel(structFieldNames)
    curField = string(structFieldNames(i));
    if isa(getfield(myStruct, curField), "double")
        fprintf("Found double %s\n", curField);
    end
end

end